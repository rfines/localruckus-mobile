".is": {
    top: 0,

    width: Ti.UI.FILL,
    height: Ti.UI.SIZE
}

".isCenter": {
    height: 50,

    bottom: 0
}

".isIndicator": {
    style: Ti.UI.ActivityIndicatorStyle.DARK
}

".isIndicator[platform=ios]": {
    style: Ti.UI.iPhone.ActivityIndicatorStyle.DARK
}

".isText": {
    wordWrap: false,
    color: '#777',
    font: {
        fontSize: 13
    }
}