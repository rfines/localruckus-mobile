			<ImageView id="detailImage" height="50" width="50"/>
		</View>
		<View class="eventDetails" width="62%">
			<Label id="name" />
		</View>
		<View class="eventTime" width="18%" right="0">
			<View class="calendarPage">
				<Label class="date"></Label>
				<Label class="day"></Label>
				<Label class="time"></Label>
			</View>
		</View>
	</TableViewRow>
</Alloy>